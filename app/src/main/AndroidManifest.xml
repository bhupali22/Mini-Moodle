 <?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.mini_moodle">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/moodle"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".Login" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Chap_cn" android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".Chap_dbms" android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".Chap_isee" android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".Chap_toc" android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".Chap_sdl" android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".About_us" android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".Google" android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".MapsActivity" android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".MainActivity" android:parentActivityName=".Login">
        </activity>
        <activity android:name=".new_sign_up" android:parentActivityName=".Login">
        </activity>
        <activity android:name=".Sub_sepm" android:parentActivityName=".Chap_sdl">
        </activity>
        <activity android:name=".Sub_cn" android:parentActivityName=".Chap_cn">
        </activity>
        <activity android:name=".Sub_dbms" android:parentActivityName=".Chap_dbms">
        </activity>
        <activity android:name=".Sub_toc" android:parentActivityName=".Chap_toc">
        </activity>
        <activity android:name=".Sub_isee" android:parentActivityName=".Chap_isee">
        </activity>

        <activity android:name=".pdf_sdl" android:parentActivityName=".Sub_sepm">
        </activity>
        <activity android:name=".pdf_cn" android:parentActivityName=".Sub_cn">
        </activity>
        <activity android:name=".pdf_dbms" android:parentActivityName=".Sub_dbms">
        </activity>
        <activity android:name=".pdf_isee" android:parentActivityName=".Sub_isee">
        </activity>
        <activity android:name=".pdf_toc" android:parentActivityName=".Sub_toc">
        </activity>

    </application>

</manifest>